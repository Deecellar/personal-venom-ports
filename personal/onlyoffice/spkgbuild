# description	: MS Office Compatible Office Tools Opensource
# homepage	: https://www.onlyoffice.com
# depends	: 

name=onlyoffice
version=6.3.1
release=1
noextract="true"
backup=""
source="https://github.com/ONLYOFFICE/appimage-desktopeditors/releases/download/v$version/DesktopEditors-x86_64.AppImage"

build() {
        mkdir -p $PKG/usr/bin
        chmod +x DesktopEditors-x86_64.AppImage
        mv DesktopEditors-x86_64.AppImage onlyoffice
	mv onlyoffice $PKG/usr/bin
}
