# description	: This project is a library for managing accounts which can be used from Qt applications. It is part of the @accounts-sso project.
# homepage	: https://gitlab.com/accounts-sso/libaccounts-qt
# depends	: qt5 libaccounts-glib doxygen

name=libaccounts-qt
version=1.16
release=1
noextract=""
backup=""
source="https://gitlab.com/accounts-sso/libaccounts-qt/-/archive/VERSION_$version/libaccounts-qt-VERSION_$version.tar.gz"

build() {
	cd $name-VERSION_$version
	qmake PREFIX=$PKG/usr
	make
	make  install -i
}
