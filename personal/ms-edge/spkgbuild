# description	: Microsoft Edge Browser Linux Version
# homepage	: 
# depends	: 

name=ms-edge
version=97.0.1072.62
release=1
noextract=""
backup=""
source="https://packages.microsoft.com/repos/edge/pool/main/m/microsoft-edge-stable/microsoft-edge-stable_$version-${release}_amd64.deb"


build() {
	ar x microsoft-edge-stable_$version-${release}_amd64.deb
	tar xvf data.tar.xz
	cp -r etc $PKG
	cp -r opt $PKG
	cp -r usr $PKG
}
